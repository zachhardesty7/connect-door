{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "standard"
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/prefer-stateless-function": 0,
    "strict": 0,
    "jsx-quotes": [
      "error",
      "prefer-single"
    ],
    "space-before-function-paren": [
      "error",
      "never"
    ]
  }
}